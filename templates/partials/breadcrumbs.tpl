<!-- IF breadcrumbs.length -->
<nav aria-class="breadcrumb">
	<ol class="breadcrumb" itemscope="itemscope" itemprop="breadcrumb" itemtype="http://schema.org/BreadcrumbList">
		{{{each breadcrumbs}}}
		<li class="breadcrumb-item"<!-- IF @last --> component="breadcrumb/current"<!-- ENDIF @last --> itemscope="itemscope" itemprop="itemListElement" itemtype="http://schema.org/ListItem" <!-- IF @last -->class="active"<!-- ENDIF @last -->>
			<meta itemprop="position" content="@index" />
			<!-- IF !@last --><a href="{breadcrumbs.url}" itemprop="item"><!-- ENDIF !@last -->
				<span itemprop="name">
					{breadcrumbs.text}
					<!-- IF @last -->
					<!-- IF !feeds:disableRSS -->
					<!-- IF rssFeedUrl --><a target="_blank" href="{rssFeedUrl}" itemprop="item"><i class="fa fa-rss-square"></i></a><!-- ENDIF rssFeedUrl --><!-- ENDIF !feeds:disableRSS -->
					<!-- ENDIF @last -->
				</span>
			<!-- IF !@last --></a><!-- ENDIF !@last -->
		</li>
		{{{end}}}
	</ol>
</nav>
<!-- ENDIF breadcrumbs.length -->
